                        </view>
                        <!-- 商品操作按钮相关 -->
                        <view class="zan-card__detail-row zan-c-gray-darker">
                          <view class="zan-card__left-col zan-c-red">浏览量：1 关注：23</view>
                        </view>
                      </view>
                    </view>
                    <view class='oper'>
