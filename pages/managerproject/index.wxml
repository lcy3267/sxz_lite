                      <view class="zan-card__thumb">
                        <image class="zan-card__img" src="{{uesrinfo.avatarUrl}}" mode="aspectFit"></image>
                      </view>
                      <!-- 卡片右侧详情描述 -->
                      <view class="zan-card__detail">
